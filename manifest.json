{
  "manifest_version": 2,
  "name": "SecurePro Extension",
  "version": "1.0",
  "description": "A simple security tool for demonstration",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "SecurePro"
  },
  "permissions": [
    "tabs",     
    "identity"  
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self'; object-src 'none';"
}

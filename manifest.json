{
  "manifest_version": 2,
  "key" : "",
  "name": "SecurePro Extension",
  "version": "1.0",
  "description": "A simple security tool for demonstration",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "SecurePro"
  },
  "permissions": [
    "tabs",     
    "identity"  
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self'; object-src 'none'; script-src-elem 'self' 'unsafe-inline' https://www.gstatic.com https://apis.google.com;"
}
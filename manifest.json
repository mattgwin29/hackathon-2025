{
  "manifest_version": 2,
  "key" : "",
  "name": "3 Way Authentication",
  "version": "1.0",
  "description": "CyberFrat Password Manager is a secure and innovative browser extension that generates unique, strong passwords for your online accountsâ€”without storing your original password. Inspired by the 2022 LastPass security breach, CyberFrat uses a 3-way authentication method to enhance security, allowing users to retrieve the same password for a website using a single master password. This eliminates the risks of centralized password storage while ensuring strong, site-specific credentials.",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "3FA",
    "default_icon": "icons/logo48.png"
  },
  "icons": {
  "48": "icons/logo48.png",
  "128": "icons/logo128.png"
},
  "permissions": [
    "tabs",     
    "identity"  
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
"content_security_policy": 
    "script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.gstatic.com https://apis.google.com https://cdnjs.cloudflare.com; object-src 'none';"
}

